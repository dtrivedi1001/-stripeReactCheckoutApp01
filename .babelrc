{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    "@babel/preset-flow"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread",
    [
      "transform-es2015-classes", {"loose":  true}
    ],
    [
      "@babel/plugin-proposal-class-properties"
    ]
  ]
}
